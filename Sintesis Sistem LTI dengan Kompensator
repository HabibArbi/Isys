clear all;
close all;

nama= 'your name'

sistem2 = tf([], []);
sistem4 = tf([], []);
sistem5 = sistem2*sistem4;
kompensator = tf([], [])
sistem6 = sistem5*kompensator
 
figure(1);
bode(sistem6, kompensator, sistem5);
legend('sistem6', 'kompensator', 'sistem5');
figure(2);
pzmap(sistem6, kompensator, sistem5);
legend('sistem6', 'kompensator', 'sistem5');
hold on;
